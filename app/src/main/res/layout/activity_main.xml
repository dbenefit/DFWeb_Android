<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <EditText
        android:id="@+id/edit_UA"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:hint="请设置UserAgent"
        android:imeOptions="actionDone"
        android:inputType="text" />


    <EditText
        android:id="@+id/edit_url"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:imeOptions="actionDone"
        android:inputType="text"
        android:text="https://aaronlianggq.github.io/web_sdk_demo.html" />

    <TextView
        android:id="@+id/tv_goWebview"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_gravity="center"
        android:background="#0000ff"
        android:gravity="center"
        android:text="跳转webview"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/tv_Scan"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_gravity="center"
        android:layout_marginTop="20dp"
        android:background="#0000ff"
        android:gravity="center"
        android:text="扫码"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/tv_location"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_gravity="center"
        android:layout_marginTop="20dp"
        android:background="#0000ff"
        android:gravity="center"
        android:text="定位-全局callback"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/tv_location1"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_gravity="center"
        android:layout_marginTop="20dp"
        android:background="#0000ff"
        android:gravity="center"
        android:text="定位-局部变量callback"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/tv_location_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:gravity="center" />

    <TextView
        android:id="@+id/tv_openFragment"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_gravity="center"
        android:layout_marginTop="20dp"
        android:background="#0000ff"
        android:gravity="center"
        android:text="跳转fragmentWebview"
        android:textColor="@color/white" />
</LinearLayout>